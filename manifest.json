{
  "manifest_version": 3,
  "name": "Auto Clicker",
  "version": "1.1",
  "description": "Specify xpaths to auto click from next time",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "*://*.accounts.google.com/*",
    "*://*.phonepe.com/*"
  ],
  "icons": {
    "48": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "scripts/serviceWorker.js"
  }
}